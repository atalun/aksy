<deviceinfo version="0.2">
  <device>
  <match key="info.bus" string="usb">
    <match key="usb.vendor_id" int="2536">
      <match key="usb.product_id" int="95">
        <append key="info.callouts.add" type="strlist">set-permissions</append>
      </match>
    </match>
  </match>
  </device>
</deviceinfo>

