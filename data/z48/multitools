BYTE
\x1B	\x51	get_target	Get target (part/channel)	BYTE
WORD
\x1B	\x52	get_destination	Get destination	BYTE
WORD
\x1B	\x53	get_change_type	Get Change type (0: replace, 1: offset)	BYTE
BYTE
