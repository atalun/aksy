\x0F	keygroup_zone_get_param	Keygroup Zone Get Parameter
\x01	get_zone_sample	Get Zone Sample (Data1=zone number 1-4)	BYTE
STRING
\x02	get_zone_level	Get Zone Level <Reply> = level in 10×dB	BYTE
LEVEL
\x03	get_zone_pan	Get Zone Pan (0-100)	BYTE
BYTE
\x04	get_zone_output	Get Zone Output(0-15)	BYTE
BYTE
\x05	get_zone_filter		Get Zone Filter (0 ­ ±100)
SBYTE
\x06	get_zone_tune	Get Zone Cents Tune(0 ­ ±3600)
SBYTE
\x07	get_keyboard_track	Get Zone Keyboard Track	BYTE
BOOL
\x08	get_playback	Get Zone Playback	BYTE	
BYTE
\x09	get_mod_start		Get Zone ModStart(0 ­ ± 99 99)	BYTE
SWORD
\x0A	get_zone_low_velocity	Get Zone Low Velocity BYTE
BYTE
\x0B	get_zone_high_velocity	Get Zone High Velocity BYTE
BYTE
\x0C	get_zone_mute	Get Zone Mute BYTE
BOOL
\x0D	get_zone_solo	Get Zone Solo BYTE
BOOL
