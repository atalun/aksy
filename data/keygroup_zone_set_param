\x0E	keygroup_zone_set_param	Keygroup Zone Set Parameter
\x01	Set Zone Sample <Data2...0> = name of sample to assign to zone. (0, 1­4)	BYTE	STRING

\x02	Set Zone Level <Data1> = Zone number, <Data2> = level in 10xdB (0, 1­4)	BYTE	SWORD

\x03	Set Zone Pan/Balance <Data2> = Pan/Bal where (0­100 = L50­R50);	BYTE	BYTE

\x04	Set Zone Output <Data2> = output, where 0=MULTI, 1 = L/R; 2­5 = op1/2­op7/8; 6­15 = L, R, op1-op8	BYTE	BYTE

\x05	Set Zone Filter	BYTE	SBYTE

\x06	Set Zone Cents Tune	BYTE	SWORD

\x07	Set Zone Keyboard Track <Data2> =  	BYTE	BYTE	BYTE

\x08	Set Zone Playback <Data2> = mode, where 0=NO LOOPING, 1=ONE SHOT 2=LOOP IN REL, 3=LOOP UNTIL REL, 4=LIRRETRIG, 5=PLAYRETRIG, 6=AS SAMPLE	BYTE	BYTE

\x09	Set Zone ModStart	BYTE	SWORD

\x0A	Set Zone Low Velocity	BYTE	BYTE

\x0B	Set Zone High Velocity	BYTE	BYTE

\x0C	Set Zone Mute <Data2> = (0=OFF, 1=ON)	BYTE	BYTE

\x0D	Set Zone Solo <Data2> = (0=OFF, 1=ON) 	BYTE	BYTE
