\x12	keygroup_set_param	Keygroup Set Parameter (should be merged with get_param
\x01	Set Group ID	BYTE

\x02	Set Keygroup Edit Mode <Data2> = (0=SINGLE, 1=ALL, 2=ADD)	BYTE

\x04	Set Low Note	BYTE

\x05	Set High Note	BYTE

\x06	BYTE(0,	1­64)	Set Mute Group <Data1> = (0=OFF, 1­64=value)

\x07	Set FX override <Data1> = (0=OFF, 1=A, 2=B, 3=C, 4=D, 5=AB, 6=CD, 7=MULTI)	BYTE

\x08	Set FX Send Level <Data1> = level in 10×dB (-600­+60)	SWORD

\x09	Set Zone Crossfade <Data1> = (0=OFF, 1=VELOCITY, 2=REAL-TIME)	BYTE

\x0A	Set Crossfade type <Data1> = (0=LIN, 1=EXP, 2=LOG)	BYTE

\x0E	Set Polyphony	BYTE

\x0F	Set Zone Crossfade Source Controller Number	(only used when Zone Crossfade Source is MIDI CTRL)	BYTE

\x10	Set Cents Tune (0 ­ ±3600)	BYTE
	
\x11	Set Keygroup Level <Data1> = value in 10×dB	SWORD
	
\x18	Set Play Trigger <Data1> = (0=NOTE ON, 1=NOTE OFF)

\x19	Set Play Trigger Velocity <Data1> = (0=ON VEL, 1=OFF VEL, 2­129=0­127)	BYTE

\x1A 	Set Play Toggle Note <Data1> = (0=OFF, 1=ON)	BYTE

\x20	Set Filter Mode	(0=NORMAL, 1=TRIPLE(1), 2=TRIPLE(2), 3=TRIPLE(3)), type	BYTE	BYTE

\x21	Set Filter Cutoff Frequency	BYTE	BYTE

\x22	Filter Resonance	BYTE	BYTE

\x23	Set Filter Attenuation (one setting for all filters) <Data1> = (0, 1, 2, 3, 4, 5 = 0dB, 6dB, 12dB, 18dB, 24dB, 30dB)	BYTE
	
\x30	Set Envelope Rate 1 (for AMP = Attack)(0=AMP, 1=FILTER, 2=AUX)	BYTE	BYTE
		
\x31	Set Envelope Level 1 (FILTER and AUX only)	BYTE	BYTE

\x32	Set Envelope Rate 2 (for AMP = Decay)	BYTE	BYTE

\x33	Set Envelope Level 2 (for AMP = Sustain)	BYTE	BYTE
	
\x34	Set Envelope Rate 3 (for AMP = Release)	BYTE	BYTE

\x35	Set Envelope Level 3 (FILTER and AUX only)	BYTE	BYTE

\x36	Set Envelope Rate 4 (FILTER and AUX only)	BYTE	BYTE
	
\x37	Set Envelope Level 4 (FILTER and AUX only)	BYTE	BYTE

\x42	Set Envelope Reference (FILTER and AUX only)	BYTE	BYTE

\x43	Set Attack Hold <Data1> = (0=OFF, 1=ON) (AMP only)

\x50	Set LFO Rate (0: LFO1 1: LFO2), rate	BYTE	BYTE

\x51	Set LFO Delay <Data2> = delay	BYTE	BYTE
		
\x52	Set LFO Depth <Data2> = depth	BYTE	BYTE

\x53	Set LFO Waveform <Data2> = waveform, (see Table 23)	BYTE	BYTE

\x54	Set LFO Phase	BYTE	BYTE

